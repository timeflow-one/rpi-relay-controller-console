{
  "compilerOptions": {
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "baseUrl": ".",
    "checkJs": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "paths": {
      "@/*": [
        "src/*",
      ],
      "#/*": [
        "./*",
      ],
    },
  },
  "exclude": [
    "node_modules"
  ]
}
